{
  "content_scripts": [
    {
      "js": [
        "extension.js",
        "autopagerize.user.js"
      ], 
      "matches": [
        "http://*/*", 
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ], 
  "description": "A browser Extension for auto loading paginated web pages. AutoPagerize use in many web site, and provide efficiently web browsing.",
  "id": "38C37D51986565D5334E62EB0D056C10A68D69C7", 
  "name": "AutoPagerize", 
  "permissions": [
    "http://wedata.net/*",
    "tabs"
  ], 
  "background_page": "background.html",
  "options_page": "options.html",
  "version": "0.2.1",
  "icons": {
     "16":  "icons/icon16.png",
     "32":  "icons/icon32.png",
     "48":  "icons/icon48.png",
     "128": "icons/icon128.png"
  },
  "page_action": {
    "default_title": "Autopagerize for Chrome",
    "popup": "popup.html",
    "default_icon": "icons/icon16.png"
  },
  "default_locale": "en"
}
